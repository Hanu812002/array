void sort012(int a[], int n)
    {  int x=0,y=0,z=0;
        for(int i=0;i<n;i++){
            if(a[i]==0)
            x++;
           else if(a[i]==1)
            y++;
           else  if(a[i]==2)
            z++;
        }
        int q=0;
        for(int i=0;i<x;i++)
        a[q++]=0;
        
        for(int i=0;i<y;i++)
        a[q++]=1;
        
        for(int i=0;i<z;i++)
        a[q++]=2;
    }
